<Page x:Class="Torq.WPF.Views.LoginPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
        Title="Log In" Height="124.894" Width="269.915">
    <Grid>
        <Label Content="Username" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0"  VerticalAlignment="Top" Width="120"/>
        <Label Content="Password" HorizontalAlignment="Left" Height="23" Margin="10,38,0,0" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="UserName"  HorizontalAlignment="Left" Height="23" Margin="135,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="Password" HorizontalAlignment="Left" Height="23" Margin="135,38,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button Click="OnLogin" Content="Log In" HorizontalAlignment="Left" Margin="180,66,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Click="OnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="71,66,0,0" VerticalAlignment="Top" Width="104"/>
    </Grid>
</Page>
